<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--address bean-->
    <bean class="com.spring.core.p6.autowire.xml.Address" name="address">
        <property name="street" value="Bhashani Street"/>
        <property name="city" value="Tangail"/>
    </bean>
    <bean class="com.spring.core.p6.autowire.xml.Address" name="address2">
        <property name="street" value="Kagmari Street"/>
        <property name="city" value="Tangail"/>
    </bean>

    <bean class="com.spring.core.p6.autowire.xml.Employee" name="employee1" autowire="constructor"/>

</beans>